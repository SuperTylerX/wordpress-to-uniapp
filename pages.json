			"path": "pages/post/post"
		},
		{
			"path": "pages/center/center",
			"style": {
				"navigationBarTitleText": "",
				"navigationBarBackgroundColor": "#FFFFFF",
				"app-plus": {
					"bounce": "none"
				}
			}
		},
		{
			"path": "pages/login/login",
			"style": {
				"navigationBarTitleText": "",
				"navigationBarBackgroundColor": "#FFFFFF",
				"app-plus": {
					"bounce": "none"
				}
			}
		},
		{
			"path": "pages/webpage/webpage",
			"style": {
